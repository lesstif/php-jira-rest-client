  "keywords": [
    "jira",
    "rest",
    "jira-php",
    "jira-rest"
  ],
  "require": {
    "php": ">=5.6",
    "silex/silex": "~1.3",
    "netresearch/jsonmapper": "~0.11",
    "symfony/http-kernel": "~3.0",
    "guzzlehttp/guzzle": "~6.2",
    "monolog/monolog": "~1.19"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.2|~4.8"
  },
  "license": "Apache 2.0",
  "authors": [
    {
      "name": "Vlad Buhanevich",
      "email": "vlad@ecomitize.com",
      "homepage": "http://ecomitize.com/"
    },
    {
      "name": "KwangSeob Jeong",
      "email": "lesstif@gmail.com",
      "homepage": "http://lesstif.com/"
    }
  ],
  "autoload": {
    "psr-4": {
      "JiraRestApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JiraRestApi\\Tests\\": "tests/unit/"
    }
  }
}
